{
    "name": "zidbih/laravel-deadlock",
    "description": "Make temporary Laravel workarounds expire and fail CI when ignored.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "nikic/php-parser": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Zidbih\\Deadlock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zidbih\\Deadlock\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zidbih\\Deadlock\\DeadlockServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "zidbih",
            "email": "medmahmoudhdaya@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
